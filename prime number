import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter n value:");
        int n=sc.nextInt();
        boolean prime=true;
        for(int i=2;i<n;i++)
        {
            if(n%i==0){
                prime=false;
                break;
            }
        }
        if(prime==true)
        {
          System.out.println("the given n " +n+ " is prime");  
        }
        else
        System.out.println("the given n "+n+" is not prime");
    }
        
}
